<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false" android:drawable="@drawable/angel_border_selected"/>
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#FFF58080" />
            <stroke android:width="1dp" android:color="#FFF58080" />
            <corners android:radius="5dp" />
        </shape>
    </item>
</selector>