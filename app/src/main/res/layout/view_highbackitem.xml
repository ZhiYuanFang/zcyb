<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <TextView
        android:id="@+id/textView3"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:background="@color/tv_line" />

    <TextView
        android:id="@+id/textView141"
        android:layout_width="1dp"
        android:layout_height="match_parent"
        android:layout_alignParentStart="true"
        android:layout_centerVertical="true"
        android:background="@color/tv_line" />

    <TextView
        android:id="@+id/textView143"
        android:layout_width="1dp"
        android:layout_height="match_parent"
        android:layout_alignParentEnd="true"
        android:layout_centerVertical="true"
        android:background="@color/tv_line" />

    <TextView
        android:id="@+id/textView144"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:background="@color/tv_line" />

    <RelativeLayout
        android:id="@+id/contain_img"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/textView144"
        android:layout_below="@+id/textView3"
        android:layout_toEndOf="@+id/textView141"
        android:layout_toStartOf="@+id/textView143">

    </RelativeLayout>
</RelativeLayout>