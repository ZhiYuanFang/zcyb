<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/bg_White"
    android:orientation="vertical">

    <RelativeLayout
        android:id="@+id/relativeLayout13"
        android:layout_width="match_parent"
        android:layout_height="@dimen/title_height"
        android:paddingTop="25dp">

        <RelativeLayout
            android:id="@+id/btn_back"
            android:layout_width="30dp"
            android:layout_height="44dp">

            <com.wzzc.other_view.extendView.ExtendImageView
                android:id="@+id/eiv_back"
                android:layout_width="11dp"
                android:layout_height="20dp"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:background="@drawable/arrow_right" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="94dp"
            android:layout_height="match_parent"
            android:layout_centerHorizontal="true">

            <com.wzzc.other_view.extendView.ExtendImageView
                android:id="@+id/eiv_title"
                android:layout_width="94dp"
                android:layout_height="20dp"
                android:layout_centerHorizontal="true"
                android:layout_centerInParent="true"
                android:layout_marginBottom="9dp"
                android:layout_marginTop="9dp"
                android:background="@drawable/tb_reback_title" />
        </RelativeLayout>

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/basicLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/relativeLayout13"
        android:paddingBottom="15dp">

        <ListView
            android:id="@+id/listView"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentTop="true"
            android:divider="@color/bg_White"
            android:dividerHeight="43dp"
            android:scrollbars="none" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/contain_fragment"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/relativeLayout13"
        android:layout_marginEnd="100dp"
        android:layout_marginStart="45dp"
        android:layout_marginTop="41dp">

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/touchLayout"
        android:layout_width="@dimen/TouchLayoutWidth"
        android:layout_height="match_parent"
        android:layout_below="@+id/relativeLayout13">

    </RelativeLayout>

</RelativeLayout>