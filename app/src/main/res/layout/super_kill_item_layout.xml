<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="187dp"
    android:background="@color/bg_White">

    <RelativeLayout
        android:id="@+id/layout_title"
        android:layout_width="158dp"
        android:layout_height="match_parent">

        <com.wzzc.other_view.extendView.ExtendImageView
            android:id="@+id/ssk_left"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="12dp"
            android:layout_marginTop="35dp"
            android:layout_weight="1"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/tv_hour"
                android:layout_width="19dp"
                android:layout_height="16dp"
                android:layout_weight="1"
                android:background="@color/bg_Black"
                android:gravity="center"
                android:singleLine="true"
                android:text="00"
                android:textColor="@color/tv_White"
                android:textSize="12sp" />

            <TextView
                android:layout_width="7dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@color/tv_White"
                android:gravity="center"
                android:text=":"
                android:textColor="@color/tv_Black"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/tv_min"
                android:layout_width="19dp"
                android:layout_height="16dp"
                android:layout_weight="1"
                android:background="@color/bg_Black"
                android:gravity="center"
                android:singleLine="true"
                android:text="28"
                android:textColor="@color/tv_White"
                android:textSize="12sp" />

            <TextView
                android:layout_width="7dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@color/tv_White"
                android:gravity="center"
                android:text=":"
                android:textColor="@color/tv_Black"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/tv_sec"
                android:layout_width="19dp"
                android:layout_height="16dp"
                android:layout_weight="1"
                android:background="@color/bg_Black"
                android:gravity="center"
                android:singleLine="true"
                android:text="18"
                android:textColor="@color/tv_White"
                android:textSize="12sp" />
        </LinearLayout>
    </RelativeLayout>

    <TextView
        android:id="@+id/textView118"
        android:layout_width="1dp"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        android:layout_toEndOf="@+id/layout_title"
        android:background="@color/tv_line" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_centerVertical="true"
        android:layout_toEndOf="@+id/textView118"
        android:orientation="vertical"
        android:weightSum="2">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="horizontal"
            android:weightSum="2">

            <com.wzzc.other_view.extendView.ExtendImageView
                android:id="@+id/ssk_right_0"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1" />

            <TextView
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:layout_weight="0"
                android:background="@color/tv_line" />

            <com.wzzc.other_view.extendView.ExtendImageView
                android:id="@+id/ssk_right_1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1" />
        </LinearLayout>

        <TextView
            android:id="@+id/textView265"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_weight="0"
            android:background="@color/tv_line" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="horizontal"
            android:weightSum="2">

            <com.wzzc.other_view.extendView.ExtendImageView
                android:id="@+id/ssk_right_2"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                tools:ignore="NestedWeights" />

            <TextView
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:layout_weight="0"
                android:background="@color/tv_line" />

            <com.wzzc.other_view.extendView.ExtendImageView
                android:id="@+id/ssk_right_3"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1" />
        </LinearLayout>
    </LinearLayout>
</RelativeLayout>