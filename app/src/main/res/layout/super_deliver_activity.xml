<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent">



    <RelativeLayout
        android:id="@+id/layout_contain_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/include9">

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/contain_select_fragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/include9"
        android:layout_marginTop="111dp">

    </RelativeLayout>

    <include
        layout="@layout/super_deliver_bottom"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:id="@+id/include9" />

    <RelativeLayout
        android:id="@+id/touchLayout"
        android:layout_width="@dimen/TouchLayoutWidth"
        android:layout_height="match_parent"
        android:layout_above="@+id/include9"
        android:layout_alignTop="@+id/contain_select_fragment">

    </RelativeLayout>
</RelativeLayout>