<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#ffffff">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="382dp"
        android:orientation="vertical">

        <RelativeLayout
            android:id="@+id/lab_relay"
            android:layout_width="match_parent"
            android:layout_height="65dp">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/lab_number"
                    android:layout_width="52dp"
                    android:layout_height="52dp"
                    android:layout_marginLeft="10dp"
                    android:background="#3fffffff"
                    android:gravity="center"
                    android:textColor="#ffffff"
                    android:textSize="18sp" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/lab_title"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:textColor="#ffffff"
                        android:textSize="16sp" />

                    <TextView
                        android:id="@+id/lab_title1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:textColor="#ffffff"
                        android:textSize="16sp" />
                </LinearLayout>
            </LinearLayout>

            <TextView
                android:id="@+id/lab_more"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_alignParentRight="true"
                android:layout_marginRight="10dp"
                android:gravity="center_vertical"
                android:text="MORE+"
                android:textColor="#ffffff"
                android:textSize="16sp" />
        </RelativeLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="197dp"
            android:orientation="horizontal"
            android:padding="10dp">

            <com.wzzc.other_view.extendView.ExtendImageView
                android:id="@+id/img_icon"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:tag="6"
                android:layout_weight="3.1" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:id="@+id/lab_thisnumber1"
                        android:tag="0"
                        android:gravity="center"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:layout_marginLeft="10dp"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/tv_name"
                            android:layout_width="match_parent"
                            android:layout_height="25dp"
                            android:textSize="16sp"/>

                        <RelativeLayout
                            android:layout_marginTop="5dp"
                            android:layout_width="match_parent"
                            android:layout_height="42dp">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_toStartOf="@+id/img_icon1"
                                android:orientation="vertical"
                                android:weightSum="2">

                                <TextView
                                    android:id="@+id/lab_name1_1"
                                    android:layout_width="wrap_content"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:singleLine="true"
                                    android:textSize="12sp" />

                                <TextView
                                    android:id="@+id/lab_money"
                                    android:layout_width="wrap_content"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:background="@drawable/bg_circle_red"
                                    android:gravity="center"
                                    android:padding="@dimen/OnepurchaseIndexSmallPadding"
                                    android:textColor="@color/tv_Red"
                                    android:textSize="12sp"
                                    android:visibility="gone" />
                            </LinearLayout>
                            <com.wzzc.other_view.extendView.ExtendImageView
                                android:id="@+id/img_icon1"
                                android:layout_width="42dp"
                                android:layout_alignParentRight="true"
                                android:layout_height="42dp"/>
                        </RelativeLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/lab_thisnumber2"
                        android:tag="1"
                        android:gravity="center"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:layout_marginLeft="10dp"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/lab_name1"
                            android:layout_width="match_parent"
                            android:layout_height="25dp"
                            android:textSize="16sp"/>

                        <RelativeLayout
                            android:layout_marginTop="5dp"
                            android:layout_width="match_parent"
                            android:layout_height="42dp">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_toStartOf="@+id/img_icon2"
                                android:orientation="vertical"
                                android:weightSum="2">

                                <TextView
                                    android:id="@+id/lab_name2_1"
                                    android:layout_width="wrap_content"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:singleLine="true"
                                    android:textSize="12sp" />

                                <TextView
                                    android:id="@+id/lab_money1"
                                    android:layout_width="wrap_content"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:background="@drawable/bg_circle_red"
                                    android:gravity="center"
                                    android:padding="@dimen/OnepurchaseIndexSmallPadding"
                                    android:textColor="@color/tv_Red"
                                    android:textSize="12sp"
                                    android:visibility="gone" />
                            </LinearLayout>


                            <com.wzzc.other_view.extendView.ExtendImageView
                                android:id="@+id/img_icon2"
                                android:layout_width="42dp"
                                android:layout_alignParentRight="true"
                                android:layout_height="42dp"/>
                        </RelativeLayout>
                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:id="@+id/lab_thisnumber3"
                        android:tag="2"
                        android:gravity="center"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:layout_marginLeft="10dp"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/lab_name2"
                            android:layout_width="match_parent"
                            android:layout_height="25dp"
                            android:textSize="16sp"/>

                        <RelativeLayout
                            android:layout_marginTop="5dp"
                            android:layout_width="match_parent"
                            android:layout_height="42dp">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_toStartOf="@+id/img_icon3"
                                android:orientation="vertical"
                                android:weightSum="2">

                                <TextView
                                    android:id="@+id/lab_name3_1"
                                    android:layout_width="wrap_content"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:singleLine="true"
                                    android:textSize="12sp" />

                                <TextView
                                    android:id="@+id/lab_money2"
                                    android:layout_width="wrap_content"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:background="@drawable/bg_circle_red"
                                    android:gravity="center"
                                    android:padding="@dimen/OnepurchaseIndexSmallPadding"
                                    android:singleLine="true"
                                    android:textColor="@color/tv_Red"
                                    android:textSize="12sp"
                                    android:visibility="gone" />
                            </LinearLayout>
                            <com.wzzc.other_view.extendView.ExtendImageView
                                android:id="@+id/img_icon3"
                                android:layout_width="42dp"
                                android:layout_alignParentRight="true"
                                android:layout_height="42dp"/>
                        </RelativeLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/lab_thisnumber4"
                        android:tag="3"
                        android:gravity="center"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:layout_marginLeft="10dp"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/lab_name3"
                            android:layout_width="match_parent"
                            android:layout_height="25dp"
                            android:textSize="16sp"/>

                        <RelativeLayout
                            android:layout_marginTop="5dp"
                            android:layout_width="match_parent"
                            android:layout_height="42dp">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_toStartOf="@+id/img_icon4"
                                android:orientation="vertical"
                                android:weightSum="2">

                                <TextView
                                    android:id="@+id/lab_name4_1"
                                    android:layout_width="wrap_content"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:singleLine="true"
                                    android:textSize="12sp" />

                                <TextView
                                    android:id="@+id/lab_money3"
                                    android:layout_width="wrap_content"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:background="@drawable/bg_circle_red"
                                    android:gravity="center"
                                    android:padding="@dimen/OnepurchaseIndexSmallPadding"
                                    android:singleLine="true"
                                    android:textColor="@color/tv_Red"
                                    android:textSize="12sp"
                                    android:visibility="gone" />
                            </LinearLayout>
                            <com.wzzc.other_view.extendView.ExtendImageView
                                android:id="@+id/img_icon4"
                                android:layout_width="42dp"
                                android:layout_alignParentRight="true"
                                android:layout_height="42dp"/>
                        </RelativeLayout>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="110dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:orientation="horizontal">

            <com.wzzc.other_view.extendView.ExtendImageView
                android:id="@+id/img_icon5"
                android:tag="4"
                android:layout_width="match_parent"
                android:layout_height="92dp"
                android:layout_weight="1" />

            <com.wzzc.other_view.extendView.ExtendImageView
                android:id="@+id/img_icon6"
                android:tag="5"
                android:layout_width="match_parent"
                android:layout_height="92dp"
                android:layout_weight="1" />
        </LinearLayout>

    </LinearLayout>
</RelativeLayout>
