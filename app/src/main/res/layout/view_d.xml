<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <com.wzzc.other_view.extendView.ExtendImageView
        android:id="@+id/eiv_0"
        android:layout_width="match_parent"
        android:layout_height="32dp">

    </com.wzzc.other_view.extendView.ExtendImageView>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <TextView
            android:id="@+id/textView237"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/tv_line" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="96dp"
            android:orientation="horizontal">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1">

                <com.wzzc.other_view.extendView.ExtendImageView
                    android:id="@+id/eiv_11"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="5dp"
                    android:layout_marginTop="45dp"
                    android:layout_weight="1"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/tv_hour"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="@drawable/bg_circle_solid_black"
                        android:gravity="center"
                        android:padding="3dp"
                        android:text="15"
                        android:textColor="@color/tv_White"
                        android:textSize="8sp" />

                    <TextView
                        android:layout_width="6dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text=":"
                        android:textColor="@color/tv_Black"
                        android:textSize="10sp" />

                    <TextView
                        android:id="@+id/tv_min"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="@drawable/bg_circle_solid_black"
                        android:gravity="center"
                        android:padding="3dp"
                        android:text="36"
                        android:textColor="@color/tv_White"
                        android:textSize="8sp" />

                    <TextView
                        android:layout_width="6dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text=":"
                        android:textColor="@color/tv_Black"
                        android:textSize="10sp" />

                    <TextView
                        android:id="@+id/tv_sec"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="@drawable/bg_circle_solid_black"
                        android:gravity="center"
                        android:padding="3dp"
                        android:text="09"
                        android:textColor="@color/tv_White"
                        android:textSize="8sp" />
                </LinearLayout>

            </RelativeLayout>

            <TextView
                android:id="@+id/textView232"
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:layout_weight="0"
                android:background="@color/tv_line" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1">

                <com.wzzc.other_view.extendView.ExtendImageView
                    android:id="@+id/eiv_12"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" />
            </RelativeLayout>
        </LinearLayout>

        <TextView
            android:id="@+id/textView236"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/tv_line" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="96dp"
            android:orientation="horizontal">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1">

                <com.wzzc.other_view.extendView.ExtendImageView
                    android:id="@+id/eiv_21"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" />
            </RelativeLayout>

            <TextView
                android:id="@+id/textView235"
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:layout_weight="0"
                android:background="@color/tv_line" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1">

                <com.wzzc.other_view.extendView.ExtendImageView
                    android:id="@+id/eiv_22"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" />
            </RelativeLayout>
        </LinearLayout>
    </LinearLayout>

</LinearLayout>